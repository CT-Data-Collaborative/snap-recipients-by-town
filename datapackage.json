{
  "name": "snap-recipients-by-town",
  "datapackage_version": "1.0-beta.18",
  "title": "SNAP Recipients by Town",
  "description": "SNAP Recipients by Town reports the number and percent of households that receive SNAP, by race/ethnicity of the householder.",
  "version": "1.0",
  "licenses": [{
    "url": "http://opendatacommons.org/licenses/pddl/",
    "name": "Open Data Commons Public Domain",
    "version": "1.0",
    "id": "odc-pddl"
  }],
  "sources": [{
    "name": "uscensus",
    "web": ""
  }],
  "author":[{
    "name": "CTData Collaborative",
    "email": "info@ctdata.org",
    "web": "http://ctdata.org"
  }],
  "contributor": [],
  "resources": [{
    "path": "data/snap-recipients-by-town-2018.csv",
    "format": "csv",
    "schema": {
      "fields": [{
          "name": "Town",
          "type": "string",
          "dimension": false
        }, {
          "name": "FIPS",
          "type": "string",
          "dimension": false
        }, {
          "name": "Year",
          "type": "string",
          "dimension": false
        }, {
          "name": "Race/Ethnicity",
          "type": "string",
          "dimension": true,
          "constraints": {
            "enum": ["All",
                     "American Indian and Alaska Native Alone",
                     "Asian Alone",
                     "Black or African American Alone",
                     "Hispanic or Latino",
                     "Native Hawaiian and Other Pacific Islander",
                     "Some Other Race Alone",
                     "Two or More Races",
                     "White Alone",
                     "White Alone Not Hispanic or Latino"
            ]
          }
        }, {
          "name": "Measure Type",
          "type": "string",
          "dimension": true,
          "constraints": {
            "enum": ["Number", "Percent"]
          }
        }, {
          "name": "Variable",
          "type": "string",
          "dimension": true,
          "constraints": {
            "enum": ["SNAP Recipients", "Margins of Error"]
          }
        }, {
          "name": "Value",
          "type": "number",
          "dimension": false
        }]
      }
    }
  ],
  "last_modified": "01/28/2020",
  "ckan_extras": {
    "full_description": {
      "ckan_name": "Full Description",
      "value": "These data originate from the American Community Survey (ACS) 5-Year estimates, tables B22005A-I. The ACS collects these data from a sample of households on a rolling monthly basis. ACS aggregates samples into one-, three-, or five-year periods. CTdata.org generally carries the five-year datasets, as they are considered to be the most accurate, especially for geographic areas that are the size of a county or smaller.",
      "type": "string"
    },
